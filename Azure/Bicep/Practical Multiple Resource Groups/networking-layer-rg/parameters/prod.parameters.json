{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "southeastasia"
    },
    "enablePrivateEndpoints": {
      "value": true
    },
    "enableApplicationGateway": {
      "value": true
    },
    "enableApiManagement": {
      "value": true
    },
    "enableAppConfiguration": {
      "value": true
    },
    "enableBlobStorage": {
      "value": true
    },
    "enableCosmosDb": {
      "value": true
    },
    "enableServiceBus": {
      "value": true
    },
    "vnetName": {
      "value": "practicalmrg-vnet-prod"
    },
    "apiManagementName": {
      "value": "practicalmrg-apim-prod"
    },
    "publisherEmail": {
      "value": "prod@practical.devsecops"
    },
    "publisherName": {
      "value": "Practical DevSecOps Production Team"
    },
    "customerPublicWebAppName": {
      "value": "practicalmrg-customer-public-prod"
    },
    "customerSiteWebAppName": {
      "value": "practicalmrg-customer-site-prod"
    },
    "adminPublicWebAppName": {
      "value": "practicalmrg-admin-public-prod"
    },
    "adminSiteWebAppName": {
      "value": "practicalmrg-admin-site-prod"
    },
    "videoApiWebAppName": {
      "value": "practicalmrg-video-api-prod"
    },
    "musicApiWebAppName": {
      "value": "practicalmrg-music-api-prod"
    },
    "apiManagementSku": {
      "value": "Premium"
    },
    "apiManagementCapacity": {
      "value": 2
    },
    "enableWAF": {
      "value": true
    },
    "wafMode": {
      "value": "Prevention"
    },
    "wafRuleSetVersion": {
      "value": "3.2"
    },
    "wafRequestBodyCheck": {
      "value": true
    },
    "wafMaxRequestBodySizeInKb": {
      "value": 128
    },
    "wafFileUploadLimitInMb": {
      "value": 100
    }
  }
}
