{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "general": {
      "value": {
        "location": "southeastasia"
      }
    },
    "featureFlags": {
      "value": {
        "enablePrivateEndpoints": true,
        "enableVNetIntegration": true,
        "enableKeyVault": true,
        "enableCosmosDb": true,
        "enableSqlServer": true,
        "enableFunctionApp": true,
        "enableApiManagement": true
      }
    },
    "productService": {
      "value": {
        "name": "product-uat"
      }
    },
    "appServicePlan": {
      "value": {
        "name": "practicalmrg-product-asp-uat"
      }
    },
    "webApp": {
      "value": {
        "productApiName": "practicalmrg-product-api-uat"
      }
    },
    "functionApp": {
      "value": {
        "name": "practicalmrg-product-func-uat",
        "storageAccountName": "practicalmrgprodfuncuat"
      }
    },
    "keyVault": {
      "value": {
        "name": "practicalmrg-prod-kv-uat"
      }
    },
    "sql": {
      "value": {
        "serverName": "practicalmrg-product-sql-uat",
        "adminUsername": "productadmin",
        "databases": {
          "product": {
            "name": "PracticalMRG-PRODUCT-DB-UAT"
          }
        }
      }
    },
    "cosmos": {
      "value": {
        "accountName": "practicalmrg-product-cosmos-uat",
        "consistencyLevel": "Session",
        "enableAutomaticFailover": true,
        "databases": {
          "product": {
            "name": "PracticalMRG-PRODUCT-COSMOS-DB-UAT"
          }
        }
      }
    },
    "networking": {
      "value": {
        "baseVnetName": "practicalmrg-vnet-uat",
        "baseVnetResourceGroup": "rg-practicalmrg-uat"
      }
    },
    "apiManagement": {
      "value": {
        "name": "practicalmrg-apim-uat"
      }
    }
  }
}
