{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "southeastasia"
    },
    "featureFlags": {
      "value": {
        "enablePrivateEndpoints": true,
        "enableVNetIntegration": true,
        "enableKeyVault": true,
        "enableCosmosDb": true,
        "enableSqlServer": true,
        "enableFunctionApp": true,
        "enableApiManagement": true
      }
    },
    "productService": {
      "value": {
        "name": "product-prod",
        "appServicePlanName": "practicalpe-product-asp-prod",
        "webApp": {
          "productApiWebAppName": "practicalpe-product-api-prod"
        },
        "azureFunction": {
          "productFunctionAppName": "practicalpe-product-func-prod",
          "storageAccountName": "practicalpeproductfuncprod"
        }
      }
    },
    "sqlServer": {
      "value": {
        "serverName": "practicalpe-product-sql-prod",
        "adminUsername": "productadmin",
        "database": {
          "productDbName": "PracticalPE-PRODUCT-DB-PROD"
        }
      }
    },
    "cosmosDb": {
      "value": {
        "accountName": "practicalpe-product-cosmos-prod",
        "consistencyLevel": "Session",
        "enableAutomaticFailover": true,
        "database": {
          "productCosmosDbName": "PracticalPE-PRODUCT-COSMOS-DB-PROD"
        }
      }
    },
    "keyVault": {
      "value": {
        "productKeyVaultName": "practicalpe-product-kv-prod"
      }
    },
    "baseServices": {
      "value": {
        "vnetName": "practicalpe-vnet-prod",
        "vnetResourceGroup": "rg-practicalpe-prod",
        "apiManagementName": "practicalpe-apim-prod"
      }
    }
  }
}
